<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <PackageVersion>1.5.1</PackageVersion>
        <Authors>Akaion</Authors>
        <Description>A Windows native DLL injection library written in C# that supports several methods of injection.</Description>
        <RepositoryUrl>https://github.com/Akaion/Bleak</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <Optimize>true</Optimize>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Jupiter" Version="1.4.0" />
      <PackageReference Include="PeNet" Version="0.8.1" />
    </ItemGroup>

</Project>
